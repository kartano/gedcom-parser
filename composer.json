{
    "name": "kartano/gedcom-parser",
    "description": "GEDCOM parser library designed around the Gedcom 5.5 standard",
    "keywords": ["gedcom"],
    "homepage": "https://github.com/kartano/gedcom-parser",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Simon Mitchell",
            "email": "kartano@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.6.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": { "GedcomParser\\": [ "src/GedcomParser" ] }
    },
    "autoload-dev": {
        "psr-4": {"GedcomParser\\": ["tests/GedcomParser"] }
    }
}
